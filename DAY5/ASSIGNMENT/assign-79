SELECT RO.LOCATION,C.CUSTTYPE,SUM(P.QUANTITY) TOTALQUANTITY 
  FROM RETAILOUTLET RO INNER JOIN PURCHASEBILL P ON RO.ROID=P.ROID 
    INNER JOIN CUSTOMER C ON P.CUSTID=C.CUSTID INNER JOIN ITEM I ON P.ITEMCODE=I.ITEMCODE 
      WHERE C.GENDER = 'M' AND I.ITEMTYPE IN ('Apparels','Computer') 
        GROUP BY RO.LOCATION,C.CUSTTYPE
